{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-any": true,
    "quotemark": [true, "single"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-unused-variable": true,
    "object-literal-key-quotes": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "arrow-parens": false
  },
  "rulesDirectory": []
}
