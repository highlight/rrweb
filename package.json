{
  "name": "@highlight-run/rrweb",
  "version": "0.10.0",
  "description": "record and replay the web",
  "scripts": {
    "test": "npm run bundle:browser && cross-env TS_NODE_CACHE=false TS_NODE_FILES=true mocha -r ts-node/register test/**/*.test.ts",
    "test:watch": "PUPPETEER_HEADLESS=true npm run test -- --watch --watch-extensions js,ts",
    "repl": "npm run bundle:browser && cross-env TS_NODE_CACHE=false TS_NODE_FILES=true ts-node scripts/repl.ts",
    "bundle:browser": "cross-env BROWSER_ONLY=true rollup --config",
    "bundle": "rollup --config",
    "bundle:dev": "rollup --config --watch",
    "typings": "tsc -d --declarationDir typings",
    "dev": "webpack --watch",
    "bundle:highlight": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/highlight-run/rrweb.git"
  },
  "keywords": [
    "rrweb"
  ],
  "main": "dist/index.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "yanzhen@smartx.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rrweb-io/rrweb/issues"
  },
  "homepage": "https://github.com/rrweb-io/rrweb#readme",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/inquirer": "0.0.43",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.7",
    "@types/puppeteer": "^1.11.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^5.0.1",
    "inquirer": "^6.2.1",
    "jest-snapshot": "^23.6.0",
    "mini-css-extract-plugin": "^1.3.8",
    "mocha": "^5.2.0",
    "puppeteer": "^1.11.0",
    "rollup": "^2.3.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript": "^1.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^4.5.1",
    "typescript": "^3.9.5",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.0"
  },
  "dependencies": {
    "@types/css-font-loading-module": "0.0.4",
    "@xstate/fsm": "^1.4.0",
    "fflate": "^0.4.4",
    "mitt": "^1.1.3"
  }
}
